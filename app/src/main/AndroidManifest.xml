<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mathpresso.togethermju">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL" />

    <application
        android:name=".core.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/myongether"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity.EmailRegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity.DetailRegisterActivity" />
        <activity android:name=".RegisterActivity.FavoriteRegisterActivity" />
        <activity android:name=".StartMainActivity" />
        <activity android:name=".NoticeDetailsActivity"></activity>
    </application>

</manifest>